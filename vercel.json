{
    "version": 2,
    "builds": [
        {
            "src": "application.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.8"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/application.py"
        }
    ]
}
